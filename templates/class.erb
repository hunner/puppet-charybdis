    max_number = <%= @max_number %>;
<% if @connectfreq -%>
    connectfreq = <%= @connectfreq %>;
<% end -%>
<% if @number_per_ident -%>
    number_per_ident = <%= @number_per_ident %>;
<% end -%>
<% if @number_per_ip -%>
    number_per_ip = <%= @number_per_ip %>;
<% end -%>
